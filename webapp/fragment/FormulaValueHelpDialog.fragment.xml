<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        noDataText="{i18n>noMaterialsFoundMessage}"
        title="{i18n>formula}"
        search="onFormulaSearch"
        confirm="onFormulaClose"
        cancel="onFormulaCancel"
        multiSelect="true"
        rememberSelections="false"
        items="{
			path: 'FormulaF4Model>/results'
		}">
        <StandardListItem
            title="{FormulaF4Model>Zzhbcformula}"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>