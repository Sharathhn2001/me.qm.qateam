<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>noMaterialsFoundMessage}"
		title="{i18n>material}"
		search="onMaterialSearch"
		confirm="onMaterialClose"
		cancel="onMaterialCancel"
		multiSelect="true"
		rememberSelections="false"
		items="{
			path: 'MaterialF4Model>/results'
		}">
		<StandardListItem
			title="{MaterialF4Model>Matnr}"
			description="{MaterialF4Model>Maktx}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
