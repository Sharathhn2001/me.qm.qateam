<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        noDataText="{i18n>noMaterialsFoundMessage}"
        title="{i18n>Batch}"
        search="onBatchSearch"
        confirm="onBatchClose"
        cancel="onBatchCancel"
        multiSelect="true"
        rememberSelections="false"
        items="{
			path: 'BatchF4Model>/results'
		}">
        <StandardListItem
            title="{BatchF4Model>Charg}"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>