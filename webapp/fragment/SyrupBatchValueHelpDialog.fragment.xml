<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        noDataText="{i18n>noSyrupBatchFoundMessage}"
        title="{i18n>SyrupBatch}"
        search="onSyrupBatchSearch"
        confirm="onSyrupBatchClose"
        cancel="onSyrupBatchCancel"
        multiSelect="false"
        rememberSelections="false"
        items="{
            path: 'SyrupBatchModel>/results'
        }"
    >
        <StandardListItem
            title="{SyrupBatchModel>Userc1}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
